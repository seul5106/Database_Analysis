<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        .dataR{
            display:none;
        }
    </style>
</head>

<body>
    <h1>Third page</h1>
    
    <input class="dataR" type="textarea" value="<%=JSON.stringify(sales)%>"/>
    <table border="1" cellspacing="0" >
        <tr>
            <th>index</th>
            <th>Region</th>
            <th>Country</th>
            <th>Item Type</th>
            <th>sales Channel</th>
            <th>Order Priority</th>
        </tr>
        <% for(let i=0; i < sales.length; i++ ){%>
            <tr>
                <td style="text-align:center"><%=i+1%></td>
                <td><%=sales[i].Region%></td>
                <td><%=sales[i].Country%></td>
                <td><%=sales[i].Item%></td>
                <td><%=sales[i].Sales%></td>
                <td><%=sales[i].Order%></td>
            </tr>
        <%}%>


    </table>
    <script id="entry-template" type="text/x-handlebars-template">
       
        
    </script>
    <script>
        const dataR = document.querySelector(".dataR").value;
        console.log(JSON.stringify(dataR));
        let orderData = []; 
        for(key of dataR){
           orderData.push(dataR[key].Order)
        }

        console.log(orderData)
    </script>
</body>

</html>